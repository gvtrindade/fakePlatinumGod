{
    "name": "fakeplatinumgod",
    "version": "1.0.0",
    "description": "Quick search app for The Binding of Isaac Rebirth Items",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/icon.icns --prune=true --out=release-builds",
        "package-win": "electron-packager . D:/Documentos/Stuff/fakeplatinumgod/release-build fakePlatinumgod  --asar --platform=win32 --arch=ia32 --icon=assets/icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"fakePlatinumGod\"",
        "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/icon.png --prune=true --out=release-builds",
        "dist": "electron-builder"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gvtrindade/fakeplatinumgod.git"
    },
    "keywords": [],
    "author": "Gabriel Trindade",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/gvtrindade/fakeplatinumgod/issues"
    },
    "homepage": "https://github.com/gvtrindade/fakeplatinumgod#readme",
    "devDependencies": {
        "electron": "^8.1.1",
        "electron-builder": "^22.4.1"
    },
    "dependencies": {
        "d3": "^5.15.0",
        "electron-context-menu": "^0.16.0"
    },
    "build": {
        "appId": "fakePlatinumGod",
        "win": {
            "icon": "./assets/icons/icon.ico"
        },
        "directories": {
            "output": "release-builds/v0.8/win"
        }
    }
}